    "patch-package": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^7.9.5",
    "set-cookie-parser": "^2.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/mocha": "^8.2.3",
    "@types/node": "^22.10.6",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.4",
    "eslint": "^9.38.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "prettier": "3.6.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.46.2"
